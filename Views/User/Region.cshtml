@model IEnumerable<HLWebRole.HLServiceReference.UserDto>
@{
    ViewBag.Title = "Users " + ViewBag.REGIONNAVN;
    Layout = "~/Views/Shared/_LayoutThreeColumn.cshtml";
}
@section GeoNavigationMenuLinks
{
    <h2>
        Users - Region @ViewBag.REGIONNAVN
        | @Html.ActionLink("News", "Region", "News", new { id = @ViewBag.Region }, null)
        | @Html.ActionLink("Communities", "Region", "Community", new { id = @ViewBag.Region }, null)
        | @Html.ActionLink("Areas", "Region", "Area", new { id = @ViewBag.Region }, null)</h2>
}
@section GeoNavigationMenu
{
    @{Html.RenderAction("_RegionMenu", "GeoNavigationMenu", new { region = @ViewBag.Region });}
}
<div id="wrapper-div">
    <div id="map-div">
        @{Html.RenderPartial("_MapUserList", Model);}
    </div>
</div>
@section ItemList
{
    <ul id="item-list">
        @{Html.RenderPartial("_UserList", Model);}
    </ul>
}
